{
  "name": "@ekidenfi/ts-sdk",
  "version": "2.0.11",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekidenfi/ekiden-ts-sdk.git"
  },
  "keywords": ["ekiden", "aptos", "sdk"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekidenfi/ekiden-ts-sdk/issues"
  },
  "homepage": "https://github.com/ekidenfi/ekiden-ts-sdk#readme",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "bun build src/index.ts --outdir dist --watch",
    "build": "bun run build:esm && bun run build:cjs && bun run build:types",
    "build:esm": "bun build src/index.ts --outdir dist --format esm --sourcemap=external --minify",
    "build:cjs": "bun build src/index.ts --outfile dist/index.cjs --format cjs --sourcemap=linked --minify",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "lint": "biome check --write src",
    "lint:check": "biome check src",
    "format": "biome format --write .",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "mock": "bun run tests/serveMock.ts",
    "publish:dry": "bun publish --dry-run",
    "release": "bun run build && bun publish"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^5.1.0",
    "@noble/curves": "^1.9.2",
    "@noble/hashes": "^1.8.0",
    "bignumber.js": "^9.3.0",
    "reconnecting-websocket": "^4.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.14",
    "dotenv": "^17.2.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=22.14.0"
  }
}
